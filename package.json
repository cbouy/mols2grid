{
	"name": "mols2grid",
	"version": "2.1.0-a4",
	"description": "Custom widget for the mols2grid package",
	"keywords": [
		"rdkit",
		"cheminformatics",
		"jupyter",
		"jupyterlab",
		"anywidget",
		"widgets"
	],
	"homepage": "https://github.com/cbouy/mols2grid",
	"bugs": {
		"url": "https://github.com/cbouy/mols2grid/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/cbouy/mols2grid"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Cedric Bouysset",
		"email": "cedric@bouysset.net"
	},
	"files": [
		"mols2grid/static/**/*.{js,js.map,d.ts,json,css}",
		"src/**/*.{js,js.map,d.ts,json,css}"
	],
	"main": "mols2grid/static/widget.js",
	"scripts": {
		"dev": "npm run build -- --sourcemap=inline --watch",
		"build": "esbuild src/widget.ts --minify --format=esm --bundle --platform=node --outdir=mols2grid/static",
		"typecheck": "tsc --noEmit",
		"patch-listjs-pagination": "sed -i 's/list.listContainer.id/list.listContainer/' node_modules/list.js-fixed/src/pagination.js",
		"prebuild": "npm run patch-listjs-pagination"
	},
	"dependencies": {
		"@floating-ui/dom": "1.7.2",
		"@rdkit/rdkit": "2025.3.4-1.0.0",
		"jquery": "3.6.0",
		"list.js-fixed": "2.3.5"
	},
	"devDependencies": {
		"@anywidget/types": "^0.2.0",
		"@types/bootstrap": "^4.5.0",
		"@types/jquery": "^3.5.32",
		"@types/popper.js": "^1.10.1",
		"esbuild": "^0.25.4",
		"typescript": "^5.8.3"
	}
}
